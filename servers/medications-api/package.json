{
  "name": "@noz/medications-api",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "nizzucompile": "npm run codegen && tsc",
    "nizzuprestart": "npm run codegen",
    "nizzunizzupredev": "npm run codegen",
    "dsdspostinstall": "npm run compile",
    "start": "npm run compile && node ./dist/index.js",
    "dev": "tsx watch ./src/index.ts",
    "dev:codegen": "concurrently \"npm run dev\" \"npm run codegen\"",
    "dev:watch": "concurrently  --names \"CodeGen,TS\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\" \"npm run dev\"",
    "watch-nodemon": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"nodemon ./dist/index.js \"",
    "codegen": "graphql-codegen --config codegen.ts",
    "createIndexesInDB": "tsx ./src/createIndexes.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "4.10.4",
    "cors": "2.8.5",
    "express": "4.19.2",
    "graphql": "16.8.1",
    "mongoose": "8.3.4"
  },
  "devDependencies": {
    "@noz/eslint-config": "workspace:*",
    "@noz/utils": "workspace:*",
    "@noz/medications-core": "workspace:*",
    "@noz/scraper-medications": "workspace:*",
    "@noz/tsconfig": "workspace:*",
    "@0no-co/graphqlsp": "1.12.5",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.7",
    "@graphql-codegen/typescript-mongodb": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "4.1.0",
    "@parcel/watcher": "2.4.1",
    "@types/node": "20.4.2",
    "concurrently": "7.4.0",
    "mongoose": "8.3.4",
    "nodemon": "2.0.19",
    "typescript": "5.4.5"
  }
}
